            <inlineChoiceInteraction responseIdentifier="id8" suffle="true">
              <inlineChoice identifier="allow1">allow</inlineChoice>
              <inlineChoice identifier="allow2">allowed</inlineChoice>
              <inlineChoice identifier="allow3">will allow</inlineChoice>
            </inlineChoiceInteraction>
