            <inlineChoiceInteraction responseIdentifier="id1" suffle="true">
              <inlineChoice identifier="is1">is</inlineChoice>
              <inlineChoice identifier="is2">was</inlineChoice>
              <inlineChoice identifier="is3">will be</inlineChoice>
